os: windows
app: Microsoft Visual Studio 2022
app: devenv.exe

quick actions: key(alt-enter)
save: key(ctrl-s)
find in solution: key(alt-shift-ctrl-l)
build: key(ctrl-b)
build solution: key(ctrl-shift-b)
comment: key(ctrl-shift-/)
comment line:
	key(home)
	insert("// ")
go to definition: key(f12)
references: key(alt-2)
debug: key(f5)
start without debugging: key(ctrl-f5)
toggle breakpoint: key(f9)