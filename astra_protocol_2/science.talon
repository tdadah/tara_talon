os: windows
app: astra protocol 2
-

science: key("f3")

launch probe:
    insert("launch probe")
    key("enter")

# The following only work within the SCIENCE module
scan target: key("f9")

[display] ( details | scientific readout ):
    insert("display details")
    key("enter")

zoom in: key("f10")

zoom out: key("f11")

recenter:
	insert("recentre")
	key("enter")









